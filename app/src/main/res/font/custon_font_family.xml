<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--regular-->
    <font
        android:font="@font/hangeul_nuri_r"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/hangeul_nuri_r"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!--bold-->
    <font
        android:font="@font/hangeul_nuri_b"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/hangeul_nuri_b"
        app:fontStyle="normal"
        app:fontWeight="1000" />

    <font
        android:font="@font/samlip_hopang_che_r"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/samlip_hopang_che_r"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <font
        android:font="@font/samlip_hopang_che_outline"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/samlip_hopang_che_outline"
        app:fontStyle="normal"
        app:fontWeight="1000" />


</font-family>